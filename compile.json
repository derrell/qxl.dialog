{
  "applications": [
    {
      "title": "Dialog Package Demo",
      "class": "qxl.dialog.demo.Application",
      "theme": "qxl.dialog.theme.Theme",
      "name": "qxl.dialog.demo",
      "default": true
    },
    {
      "class": "qxl.apiviewer.Application",
      "theme": "qxl.apiviewer.Theme",
      "name": "apiviewer",
      "title": "Qooxdoo API Viewer",
      "outputPath": "apiviewer",
      "include": [
        "qx.*"
      ],
      "exclude": [
        "qx.test.*",
        "qx.module.Blocker",
        "qx.module.Placement"
      ],
      "environment": {
        "qx.icontheme": "Tango",
        "excludeFromAPIViewer": [
          "qxl.apiviewer.*"
        ]
      }
    }
  ],
  "environment": {
    "module.objectid": true
  },
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "environment": {
        "qx.debug": true
      },
      "bundle": {
        "include": [
          "qx.*"
        ],
        "exclude": [
          "qx.util.*"
        ]
      }
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en"
  ],
  "$schema": "https://raw.githubusercontent.com/qooxdoo/qooxdoo-compiler/master/source/resource/qx/tool/schema/compile-1-0-0.json"
}